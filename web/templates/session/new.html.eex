<%= form_for @conn, session_path(@conn, :create), [as: :session], fn form -> %>
  <div>
    <%= label form, :email %>
    <%= email_input form, :email %>
  </div>
  <div>
    <%= label form, :password %>
    <%= password_input form, :password %>
  </div>
  <div>
    <%= submit "Sign in" %>
  </div>
<% end %>
